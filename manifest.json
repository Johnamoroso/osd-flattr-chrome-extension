{
  "name": "Fast Flattr",
  "version": "0.5",
  "description": "This extension gives an easy way to flattr the website you are on.",
  "background_page": "background.html",
  "page_action": {
    "default_icon": "icon_128.png",
    "default_title": "Flattr this!"
  },
  "permissions": [
    "tabs",
    "https://api.flattr.com/"
  ],
  "icons": {
    "48": "icon_48.png",
    "128": "icon_128.png"
  }
}
